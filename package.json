{
  "name": "lunar",
  "version": "0.0.3",
  "author": "Marching Gray Young",
  "license": "MIT",
  "description": "The script to calculate solar term.",
  "keywords": [
    "solar",
    "tolar",
    "lunar"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --config jestconfig.json",
    "test-watch": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --config jestconfig.json",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prettier:write": "npx prettier --write ./src",
    "prettier:check": "npx prettier --check ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrayYoung/Lunar.git"
  },
  "bugs": {
    "url": "https://github.com/GrayYoung/Lunar/issues"
  },
  "homepage": "https://github.com/GrayYoung/Lunar#readme",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.2",
    "ts-node": "~10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.10",
    "ephemeris": "^2.0.0"
  },
  "publishConfig": {
    "name": "@marching/lunar",
    "registry": "https://github.com/GrayYoung/Lunar.git"
  }
}